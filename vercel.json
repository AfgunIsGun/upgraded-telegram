{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "installCommand": "npm install",
        "buildCommand": "npm run build:full"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/sign-translate/server/server.mjs"
    }
  ]
}
